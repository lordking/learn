   "source": [
    "## 解二元一次方程式\n",
    "\n",
    "$\n",
    "\\begin{aligned}\n",
    "x+y=3 \\\\\n",
    "y=4\n",
    "\\end{aligned}\n",
    "$"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 27,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "最终结果:\n",
      "{x: -1, y: 4}\n"
     ]
    }
   ],
   "source": [
    "from sympy import *\n",
    "\n",
    "x= Symbol('x')\n",
    "y = Symbol('y')\n",
    "\n",
    "expr1 = x + y - 3\n",
    "expr2 = y - 4\n",
    "\n",
    "print(\"最终结果:\")\n",
    "print(solve([expr1,expr2],[x,y]))"
   ]
  },
